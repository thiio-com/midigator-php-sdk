{
    "name": "thiio/midigator-php-sdk",
    "description": "This is a SDK built in PHP for consuming midigator api as detailed https://midigator.com/",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Thiio\\Midigator\\": "src/",
            "Thiio\\Midigator\\Models\\": "src/models/",
            "Thiio\\Midigator\\Requests\\": "src/requests/",
            "Thiio\\Midigator\\Exceptions\\": "src/exceptions/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thiio\\Midigator\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Carlos Enrique Hoy Chi",
            "email": "enrique@thiio.com"
        },
        {
            "name": "WebforceHQ",
            "email": "fordonez@webforcehq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
